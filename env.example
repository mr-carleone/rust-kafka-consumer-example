# Основной файл конфигурации
# Скопируйте этот файл в .env и измените значения под ваше окружение

# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_TOPIC=test-topic

# Consumer Configuration
CONSUMER_GROUP_ID=rust-kafka-consumer-group
CONSUMER_SESSION_TIMEOUT_MS=6000
CONSUMER_AUTO_OFFSET_RESET=earliest

# Producer Configuration
PRODUCER_MESSAGE_TIMEOUT_MS=5000
PRODUCER_RETRY_BACKOFF_MS=100
PRODUCER_MAX_RETRIES=3

# Application Configuration
RUST_ENV=development

# Logging Configuration
RUST_LOG=info
LOG_LEVEL=info
LOG_RETENTION_DAYS=3
